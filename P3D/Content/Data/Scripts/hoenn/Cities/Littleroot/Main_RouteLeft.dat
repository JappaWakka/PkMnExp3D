		@npc.moveasync(3,1)
		@player.turnto(3)
		@npc.moveasync(3,1)
		@player.turnto(2)
	:endif
	@npc.turn(3,1)
	@npc.move(3,3)
	@npc.turn(3,0)
	@npc.move(3,1)
	@npc.turn(3,2)
:endif
